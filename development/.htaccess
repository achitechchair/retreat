#Options -MultiViews
Options +FollowSymLinks
RewriteEngine On

#RewriteCond %{HTTPS} off
#RewriteRule ^(.*)$ https://%{SERVER_NAME}%{REQUEST_URI} [R=301]

IndexIgnore */*
#RewriteBase /development

RewriteRule ^index.html index.php

RewriteRule ^terms-and-conditions terms-and-conditions.php

RewriteRule ^registration-home registration-home.php
RewriteRule ^registration-step1 registration-step1.php
RewriteRule ^registration-step2 registration-step2.php
RewriteRule ^registration-step3 registration-step3.php

RewriteRule ^registration-packages registration-packages.php

RewriteRule ^registration-member-info registration-member-info.php
RewriteRule ^registration-profile-info registration-profile-info.php
RewriteRule ^registration-family-info registration-family-info.php
RewriteRule ^registration-driving registration-driving.php
RewriteRule ^registration-youth-networking-event registration-youth-networking-event.php
RewriteRule ^registration-payment-bill registration-payment-bill.php
RewriteRule ^registration-payment-options registration-payment-options.php

RewriteRule ^login-to-edit-registration login-to-edit-registration.php
RewriteRule ^registration-dashboard registration-dashboard.php

RewriteRule ^edit-registration-profile-info edit-registration-profile-info.php
RewriteRule ^edit-registration-member-info edit-registration-member-info.php
RewriteRule ^edit-registration-family-info edit-registration-family-info.php
RewriteRule ^registration-zelle-payment-details registration-zelle-payment-details.php

RewriteRule ^updates updates.php
RewriteRule ^contact-us contact-us.php
RewriteRule ^faq faq.php

RewriteRule ^thankyou/([^/]+) thankyou.php?messg=$1 [QSA]
RewriteRule ^thankyou  thankyou.php

RewriteRule ^pay-info pay-info.php

RewriteRule ^packages packages.php
RewriteRule ^hotel-info hotel-info.php
RewriteRule ^directions directions.php
RewriteRule ^youth-activities youth-activities.php
RewriteRule ^registration-statistics registration-statistics.php
RewriteRule ^program-schedule program-schedule.php



<IfModule mod_deflate.c>
	<filesMatch "\.(js|css|html|php)$">
		SetOutputFilter DEFLATE
	</filesMatch>
	<FilesMatch "\.(jpg|jpeg|png|gif|swf)$">
	    Header set Cache-Control "max-age=31536000, public"
	</FilesMatch>	
</IfModule>

<IfModule mod_expires.c>
	AddType application/font-sfnt            otf ttf
	AddType application/font-woff            woff
	AddType application/font-woff2           woff2
	AddType application/vnd.ms-fontobject    eot

	ExpiresActive on
	ExpiresDefault "access plus 2 month"
	ExpiresByType application/pdf "access plus 2 month"
	ExpiresByType application/x-shockwave-flash "access plus 2 month"
	ExpiresByType text/html "access plus 2 days"
	ExpiresByType image/jpg "access 2 month"
	ExpiresByType image/gif "access 2 month"
	ExpiresByType image/jpeg "access 2 month"
	ExpiresByType image/png "access 2 month"
	ExpiresByType text/css "access 2 month"
	ExpiresByType application/x-javascript "access plus 2 month"
	ExpiresByType text/javascript "access plus 2 month"
	ExpiresByType application/javascript "access plus 2 month"
	ExpiresByType image/x-icon "access plus 2 month"
	ExpiresByType image/icon "access plus 2 month"
	ExpiresByType application/x-icon "access plus 2 month"
	ExpiresByType application/icon "access plus 2 month"

	ExpiresByType application/font-woff "access plus 2 month" 
	ExpiresByType application/font-woff2 "access plus 2 month"
	ExpiresByType application/font-sfnt "access plus 2 month"
	ExpiresByType application/vnd.ms-fontobject "access plus 2 month"
</IfModule>


